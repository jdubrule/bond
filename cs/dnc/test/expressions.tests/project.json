{
    "name": "Bond.ExpressionsTest",
    "version": "1.0.0-*",
    "buildOptions": {
        "compile": {
            "include": [
                "../../../test/expressions/*.cs",
                "../../../test/expressions/properties/*.cs",
                "gen/*.cs"
            ]
        },
        "debugType": "portable",
        "emitEntryPoint": true,
        "nowarn": [
            "CS1591"
        ],
        "warningsAsErrors": true,
        "xmlDoc": true
    },
    "configurations": {
        "debug": {
            "buildOptions": {
                "keyFile": "../../../build/internal/bond.snk",
                "optimize": false,
                "publicSign": true
            }
        },
        "debug-delay": {
            "buildOptions": {
                "define": [
                    "DELAY_SIGN"
                ],
                "delaySign": true,
                "keyFile": "../../keys/delay-sign.snk",
                "optimize": true
            }
        },
        "release": {
            "buildOptions": {
                "keyFile": "../../../build/internal/bond.snk",
                "optimize": true,
                "publicSign": true
            }
        },
        "release-delay": {
            "buildOptions": {
                "define": [
                    "DELAY_SIGN"
                ],
                "delaySign": true,
                "keyFile": "../../keys/delay-sign.snk",
                "optimize": true
            }
        }
    },
    "dependencies": {
        "attributes": "1.0.0-*",
        "core": "1.0.0-*",
        "io": "1.0.0-*",
        "json": "1.0.0-*",
        "reflection": "1.0.0-*"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": [
                "netcoreapp1.0",
                "portable-net45+win8"
            ],
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.0.0-*",
                    "type": "platform"
                }
            }
        }
    }
}
